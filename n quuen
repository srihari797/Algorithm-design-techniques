#include<iostream>
using namespace std;
int n,row,col;
int pos[100];
bool found=false;
bool safe(int r,int c){
    for(int i=0;i<r;i++){
        if(pos[i]==c || pos[i]+i==r+c || pos[i]-i ==c-r){
            return false;
        }
        
    }
    return true;
}
void solve(int r){
    if(r==n) {
        found=true;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(pos[i]==j){
                    cout<<"Q ";
                }
                else{
                    cout<<". ";
                }
            }
            cout<<endl;
            
        }
        cout<<endl;
        return;
    }
    if(row==r){
        if(safe(r,col)){
            pos[r]=col;
            solve(r+1);
        }
        return ;
    }
    for(int i=0;i<n;i++){
       if(safe(r,i)){
           pos[r]=i;
           solve(r+1);
       }
    }
}
int main(){
    cin>>n>>row>>col;
    solve(0);
    if(!found) cout<<"no";
}
