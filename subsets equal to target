#include<iostream>
#include<vector>
using namespace std;
vector<int> c;
void  rec(int index,int arr[],vector<int> &num,int sum,int k,int count,int n){
    if(index==n){
        if(sum==k){
            count++;
        }
        c.push_back(count);
        return ;
    }
    num.push_back(arr[index]);
    sum+=arr[index];
    rec(index+1,arr,num,sum,k,count,n);
    
    sum-=arr[index];
    num.pop_back();
    rec(index+1,arr,num,sum,k,count,n);
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int k;
    cin>>k;
    vector<int> num;
    
    rec(0,arr,num,0,k,0,n);
    int a=0;
    for(int i=0;i<c.size();i++){
        if(c[i]==1){
            a++;
        }
    }
    
    
    cout<<a;
    
    
}
