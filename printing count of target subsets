Input 1 :
4 -n
1 3 5 2
6-tar
Output 1 :
2



#include <bits/stdc++.h>
using namespace std;
int rec(int index,vector<int> &arr,int tar,int sum,vector<int> &ans,int count){
    if(index==arr.size()){
        if(tar==sum){
            count++;
        }
        return count;
    }
    ans.push_back(arr[index]);
    sum+=arr[index];
  count= rec(index+1,arr,tar,sum,ans,count);
    
    ans.pop_back();
    sum-=arr[index];
     count=rec(index+1,arr,tar,sum,ans,count);
     return count;
    
}

int main() {
	// your code goes here
	int n;
	cin>>n;
	vector<int> arr(n);
	vector<int> ans;
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int tar;
	cin>>tar;
	 int c=rec(0,arr,tar,0,ans,0);
	 cout<<c<<" ";

}
