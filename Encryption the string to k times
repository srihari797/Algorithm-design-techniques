#include<iostream>
using namespace std;
int main(){
    string s;
    int k;
    getline(cin,s);
    cin>>k;
    string res="";
    
    for(int i=0;i<s.length();i++){
        char fin;
        int a=(int)(s[i]+k);
        if(a>122 && s[i]!=' '){
            
            char c=(char)(96+(k-(122-s[i])));
    
            res+=c;
        }
        else if(s[i]!=' '){
          fin=((char)s[i]+k);
         
        res+=fin;    
        }
        else{
            res+=' ';
        }
        
    }
     cout<<"Encrypted message: "<<res;
    

}


