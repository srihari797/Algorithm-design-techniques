#include<iostream>
#include<algorithm>
#include<iomanip>
#include<vector>
using namespace std;
int main(){
    vector<int> weight;
    vector<int> value;
    int amt;
    bool found=true;
    int n=0;
    while(found){
        int data,data1;
        cin>>data>>data1;
        if(data==-1 || data1==-1){
            break;
        }
        weight.push_back(data);
        value.push_back(data1);
        n++;
    }
    cin>>amt;
    double per[n];
    for(int i=0;i<n;i++){
        per[i]=(double)value[i]/weight[i];
    }
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(per[i]<per[j]){
                swap(per[i],per[j]);
                swap(weight[i],weight[j]);
                swap(value[i],value[j]);
            }
        }
    }
    double sum=0.0;
    double fra;
    for(int i=0;i<n;i++){
        if(amt>weight[i]){
            sum+=value[i];
            amt=amt-weight[i];
        }
        else{
            sum+=amt*per[i];
            fra=(double)amt/weight[i];
            fra=fra*100;
            
            break;
        }
    }
    cout<<"The maximum value of the current list is: "<<fixed<<setprecision(2)<<sum<<endl;
    cout<<fra;
}
