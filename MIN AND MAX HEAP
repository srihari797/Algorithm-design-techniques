#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
class heap{
  public:  
  vector<int> res;
  void create(int data)
  {
      res.push_back(data);
      heapifyup(res.size()-1);
  }
  void heapifyup(int index){
      int parent=(index-1)/2;
      while(index>0 && res[parent]>res[index]){
          swap(res[parent],res[index]);
          index=parent;
          parent=(index-1)/2;
      }
  }
  void display(){
      for(int i=0;i<res.size();i++){
          cout<<res[i]<<" ";
      }
  }
};
int main(){
    int n;
    cin>>n;
        heap h;
    for(int i=0;i<n;i++){
        int data;
        cin>>data;
        h.create(data);
    }
    h.display();

    
    
}
